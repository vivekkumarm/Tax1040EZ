                            </Assign.To>
                            <Assign.Value>
                              <InArgument x:TypeArguments="x:Double">
                                <mca:CSharpValue x:TypeArguments="x:Double">(W2.WagesAmount.HasValue ?W2.WagesAmount.Value:0) + TotalWages</mca:CSharpValue>
                              </InArgument>
                            </Assign.Value>
